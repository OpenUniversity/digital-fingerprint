    </div>
    <div class="row marketing" id="name-container" style="display: none;">
      <div class="col-lg-12">
        <p id="name-display"></p>
      </div>
    </div>

    <div class="row marketing" id="form-container">
      <div class="col-lg-12">
          <form class="form-signin">
            <label for="name">Insert Your name</label>
            <input class="form-control" type="text" id="name" required="required" placeholder="Your name" />
            <input type="hidden" id="hash" />
            <button id="initHashInsertion" class="btn btn-lg btn-primary btn-block" type="button" >test</button>
            <div id="result"></div>
          </form>
      </div>
    </div>

  </div>
